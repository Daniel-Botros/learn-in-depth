# include "lifo.h"
#include "stdio.h"
//APIs
LIFO_status LIFO_init (LIFO_Buf_t* lifo_buf, unsigned int* buf, unsigned int size ){
	if(buf == NULL)
		return LIFO_Null;

	lifo_buf->base = buf;
	lifo_buf->head = buf;
	lifo_buf->count = size;
	lifo_buf->count = 0;
	return LIFO_no_error;
}
