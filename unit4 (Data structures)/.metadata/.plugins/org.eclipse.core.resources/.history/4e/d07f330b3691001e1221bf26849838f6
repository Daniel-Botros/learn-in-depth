#ifndef LIFO_H_
#define LIFO_H_

//type defs

typedef struct {
	unsigned int size ;
	unsigned int count ;
	unsigned int* base ;
	unsigned int* head ;
}LIFO_Buf_t;

typedef enum {
	LIFO_no_error,
	LIFO_full,
	LIFO_empty,
	LIFO_Null,
}LIFO_status;

//APIs


#endif /* LIFO_H_ */
