#include "fifo.h"

//fifo APIS

FIFO_BUF_Status FIFO_init (FIFO_Buf_t* fifo, element_type* buf, uint32_t length){
	if (buf == NULL)
		return FIFO_Null;

	fifo->length = length;
	fifo->base = buf;
	fifo->head = buf;
	fifo->tail = buf;
	fifo->count = 0;

	return FIFO_no_error;
}
