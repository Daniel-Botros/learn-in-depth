#include "string.h"
#include "conio.h"

//effective data

struct Sdata{
	int ID;
	char name [40];
	float height;
};

struct SStudent{
	struct Sdata Student;
	struct SStudent* PNextStudent;
};

//head

struct SStudent* gpFirstStudent = NULL;

//APIs

void AddStudent();
